export PYTHONPATH=/Users/johnwitulski/witulski/git/pyB/pyB:.John-Witulskis-MacBook-Pro:pyB    

kills proB:

MACHINE del_me
VARIABLES xx
INVARIANT
 xx:NAT
INITIALISATION xx:=8
OPERATIONS
  op(ww,vv) = PRE ww:NAT & vv <= xx THEN PRE vv:NAT & vv>=xx THEN xx:= ww+vv END END
END